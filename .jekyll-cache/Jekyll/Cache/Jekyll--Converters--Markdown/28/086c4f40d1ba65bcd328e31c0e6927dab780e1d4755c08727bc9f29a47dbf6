    <span class="na">left</span><span class="pi">:</span> <span class="s">5%</span>
    <span class="na">right</span><span class="pi">:</span> <span class="s">5%</span>
    <span class="na">bottom</span><span class="pi">:</span> <span class="s">5%</span>
